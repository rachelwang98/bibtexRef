@Article{Logan1985,
  author       = {Gordon D. Logan},
  title        = {Executive control of thought and action},
  journaltitle = {Acta Psychologica},
  year         = {1985},
  volume       = {60},
  number       = {2-3},
  month        = dec,
  pages        = {193--210},
  issn         = {0001-6918},
  doi          = {10.1016/0001-6918(85)90055-1},
  abstract     = {In addition to processing information about stimuli in the environment,
	the cognitive system performs executive functions to control and
	coordinate the processes that deal with environmental stimuli. Several
	executive functions may be distinguished: (1) Making choices about
	alternative strategies for processing environmental stimuli; (2)
	constructing or instantiating a version of the chosen strategy to
	enable performance on the task; (3) controlling and coordinating
	execution of the strategy during real-time performance on the task;
	and (4) disabling or disengaging the strategy in response to changes
	in goals or changes in the task environment that make the current
	strategy inappropriate. This paper presents a selective review of
	research addressing each of these executive functions, focusing on
	the time and resources used to accomplish each one.},
  file         = {EF:Logan1985 - Executive control of thought and action.pdf:PDF},
  groups       = {Executive_function, General},
  publisher    = {Elsevier BV},
}

