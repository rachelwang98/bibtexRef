@Article{RUBIA_1999,
  author    = {KATYA RUBIA and ALAN TAYLOR and ERIC TAYLOR and JOSEPH A. SERGEANT},
  title     = {Synchronization, anticipation, and consistency in motor timing of children with dimensionally defined attention deficit hyperactivity behaviour.},
